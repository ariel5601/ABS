<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>


<ScrollPane stylesheets="@style1.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.mainViewController">
   <content>
      <BorderPane fx:id="borderPane" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="1250.0" styleClass="BorderPane" stylesheets="@style1.css">
         <top>
            <fx:include fx:id="headerComponent" source="../header/headerComponent.fxml" />
         </top>
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <fx:include fx:id="bodyAdminComponent" source="../body/bodyAdmin.fxml" />
                  <fx:include fx:id="bodyCustomerComponent" source="../body/bodyCustomer.fxml" />
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </content>
</ScrollPane>
